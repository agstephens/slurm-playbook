slurm_wn_cpus: 1
slurm_server_name: 'slurmmaster'
slurm_server_ip: '192.168.50.44'
slurm_wn_nodenames: ['{{ slurm_server_name }}']

# The following is only needed on some local private networks 
# where vagrant hosts need to be known in "/etc/hosts"
for_etc_hosts: '{{ slurm_server_ip }} {{ slurm_server_name }} {{ slurm_server_name }}.localdomain'
